@model IEnumerable<milestone2.ProductCategory>
@{
    ViewBag.Title = "Index";
}

<h2>Ultimate Cycling Center</h2>
<h4 style="font-style:italic">Browse of selection of cycling accessories</h4>

<table class="table">
    
    @foreach (var item in Model)
    {
    <tr>
        <td>@Html.ActionLink(item.Name, item.Name)</td>
    </tr>
    }
    
</table>
